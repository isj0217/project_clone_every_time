<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.everytime_mock">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".src.ApplicationClass"
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity android:name=".src.signIn.SignInActivity"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".src.signUp.SignUpAgreementActivity" />
        <activity android:name=".src.signUp.SignUpInputFormActivity" />
        <activity android:name=".src.main.MainActivity" />
        <activity android:name=".src.boards.writing.WritingActivity" />
        <activity android:name=".src.boards.in_post.InPostActivity" />
        <activity android:name=".src.main.frag_home.my_page.MyPageActivity" />
        <activity android:name=".src.boards.general_boards.free_board.FreeBoardActivity" />
        <activity android:name=".src.boards.general_boards.secret_board.SecretBoardActivity" />
        <activity android:name=".src.boards.general_boards.alumni_board.AlumniBoardActivity" />
        <activity android:name=".src.boards.general_boards.freshmen_board.FreshmenBoardActivity" />
        <activity android:name=".src.boards.hot_board.HotBoardActivity" />
        <activity android:name=".src.boards.lecture_review.RecentLectureReviewBoardActivity" />
        <activity android:name=".src.boards.lecture_review.LectureReviewInSubjectActivity" />
    </application>

</manifest>